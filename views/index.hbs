            {{/each}}

            </tbody>
        </table>

        <h2 id="discounts">Discounts</h2>

        <p>
            Opportunities to lower your camp fees (or the fees of others!)
        </p>

        <table class="table">
            <thead>
            <tr>
                <th class="name">Name</th>
                <th>Value</th>
                <th>Available To</th>
            </tr>
            </thead>
            <tbody>
            {{#each config.discounts}}
